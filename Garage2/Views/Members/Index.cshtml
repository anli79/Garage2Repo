@model IEnumerable<Garage2.Models.Member>

@{
    ViewBag.Title = "Index";
}

<h2>Registrerade medlemmar</h2>

<p>
    @Html.ActionLink("Ny medlem", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PNr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActiveMemberShip)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActiveMemberShip)
        </td>

        <td>
            @Html.ActionLink("Ändra", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detaljer", "Details", new { id=item.Id }) |
            @Html.ActionLink("Ta bort", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
